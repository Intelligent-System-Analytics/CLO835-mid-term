# exclude git and terraform sources
.git/
**/.terraform/

# Terraform lock file
*.lock.hcl

# Local .terraform directories
**/.terraform/*

# Ignore Terraform state files
*.tfstate
*.tfstate.*
*.tfstate.backup
*.tfplan

# Ignore override files as they are usually used to override resources created by Terraform
override.tf
override.tf.json
*_override.tf
*_override.tf.json

# Ignore CLI configuration files
.terraformrc
terraform.rc

# Crash log files
crash.log

# Keys
assignment2*
*.pub

